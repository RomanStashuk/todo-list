{
  "root": true,
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": "google",
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "require-jsdoc": "off",
    "max-len": [
      "error",
      {
        "code": 120
      }
    ],
    "no-unused-vars": "warn"
  },
  "ignorePatterns": [
    "webpack/",
    "node_modules/"
  ]
}
